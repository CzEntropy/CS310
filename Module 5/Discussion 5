#include <iostream>

// Function to calculate the square of a number
int square(int x) {
    return x * x; // Incorrect multiplication operator
}

// Function to print a message
void printMessage() {
    std::cout << "Hello, world!" << std::endl
}

int main() {
    int num = 5;
    std::cout << "Square of " << num << " is: " << square(num) << std::endl;
    printmessage(); //Lowercase 'message' won't call the function ' printMessage'
    return 0;
}
